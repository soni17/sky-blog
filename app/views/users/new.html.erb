<h2>Sign Up</h2>

<%= form_with model: @user, url: sign_up_path do |f|  %>

  <p>
    <%= f.label :First_Name %>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :Last_Name %>
    <%= f.text_field :last_name %>	
  </p>

  <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Sign Up" %>
  </p>
  
<% end %>